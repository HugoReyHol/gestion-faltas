<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER_LEFT" prefHeight="293.0" prefWidth="331.0" spacing="10.0" style="-fx-border-radius: 10;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="org.dam2.gestionfaltas.controller.LoginCtrll">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" text="Inicio de sesión">
      <font>
         <Font size="30.0" />
      </font>
   </Label>
   <Label layoutX="40.0" layoutY="75.0" text="Número Asignado:">
      <font>
         <Font size="14.0" />
      </font>
   </Label>
   <TextField fx:id="inNumero">
      <font>
         <Font size="14.0" />
      </font>
      <VBox.margin>
         <Insets bottom="10.0" />
      </VBox.margin>
   </TextField>
   <Label layoutX="40.0" layoutY="112.0" text="Contraseña:">
      <font>
         <Font size="14.0" />
      </font>
   </Label>
   <PasswordField fx:id="inContrasena">
      <font>
         <Font size="14.0" />
      </font>
      <VBox.margin>
         <Insets bottom="10.0" />
      </VBox.margin>
   </PasswordField>

    <Button maxWidth="1.7976931348623157E308" onAction="#onLogIn" text="Iniciar sesión">
      <font>
         <Font size="14.0" />
      </font></Button>
</VBox>
